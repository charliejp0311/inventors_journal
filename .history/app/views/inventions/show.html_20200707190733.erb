<h1>Inventions</h1>
<%= render partial: 'invention', locals: {invention: @invention} %>
<% @invention.problems.each do |prob|%>
    <%=form_for prob.solutions.build do |s| %>
        <%# <%= s.hidden_field :invention_id, :value=> invention.id %>
        <%= s.label :solution %>
        <%= s.text_field :solution %><br/>
        <%= s.submit 'Resolve'%>
    <% end %>
<% end %>
<%= render partial: 'problem_form', locals: {problem: @problem} %>
