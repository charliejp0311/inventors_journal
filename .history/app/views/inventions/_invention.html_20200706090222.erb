<h3><%= invention.title %></h3>
<b><%= invention.goal %></b><br/>
<i><%= invention.description %></i>
<br/>
<% invention.problems.each do |prob| %>
    <%= prob.problem %>
    <%= prob.solutions.each do |solut| %>
        <%= render partial: 'solution_form', locals: {solution: solut} %>
    <% end %>
<% end %>